{
  "name": "networkneuron",
  "version": "1.0.0",
  "description": "A decentralized VPN (dVPN) protocol for secure, censorship-resistant internet access",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "test": "jest",
    "build": "npm run build:client && npm run build:node",
    "build:client": "webpack --mode production",
    "build:node": "tsc",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js"
  },
  "keywords": [
    "dvpn",
    "decentralized",
    "vpn",
    "privacy",
    "censorship-resistance",
    "p2p",
    "cryptocurrency"
  ],
  "author": "NetworkNeuron Team",
  "license": "MIT",
  "dependencies": {
    "libp2p": "^0.46.0",
    "@libp2p/websockets": "^6.0.0",
    "@libp2p/tcp": "^6.0.0",
    "@libp2p/noise": "^4.0.0",
    "@libp2p/mplex": "^8.0.0",
    "@libp2p/kad-dht": "^12.0.0",
    "@libp2p/bootstrap": "^6.0.0",
    "multiformats": "^12.0.0",
    "peer-id": "^2.0.0",
    "crypto": "^1.0.1",
    "node-forge": "^1.3.1",
    "ws": "^8.14.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "winston": "^3.10.0",
    "joi": "^17.9.2",
    "uuid": "^9.0.0",
    "big.js": "^6.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/ws": "^8.5.5",
    "@types/uuid": "^9.0.2",
    "typescript": "^5.1.6",
    "jest": "^29.6.2",
    "eslint": "^8.47.0",
    "prettier": "^3.0.1",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
